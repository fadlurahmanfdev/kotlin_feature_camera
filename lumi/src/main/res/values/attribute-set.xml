<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="progress" format="float" />
    <attr name="circleRadiusRatio" format="float" />
    <attr name="centerY" format="dimension" />
    <attr name="opacity" format="float" />
    <attr name="progressColor" format="color" />
    <attr name="strokeWidth" />
    <attr name="widthLengthRatio" format="float" />
    <attr name="yTop" format="dimension" />

    <declare-styleable name="CircleProgressOverlayView">
        <!-- Specifies the progress for circle progress painter, minimum is 0.0, max is 1.0. -->
        <attr name="progress" />
        <attr name="circleRadiusRatio" />
        <attr name="centerY" />
        <attr name="strokeWidth" />
        <attr name="backgroundColor" />
        <attr name="opacity" />
        <attr name="progressColor" />
    </declare-styleable>

    <declare-styleable name="RectangleOverlayView">
        <!-- Specifies the progress for circle progress painter, minimum is 0.0, max is 1.0. -->
        <attr name="widthLengthRatio"/>
        <attr name="strokeWidth" />
        <attr name="backgroundColor" />
        <attr name="opacity" />
        <attr name="yTop"/>
    </declare-styleable>

    <declare-styleable name="SquareOverlayView">
        <!-- Specifies the progress for circle progress painter, minimum is 0.0, max is 1.0. -->
        <attr name="widthLengthRatio"/>
        <attr name="strokeWidth" />
        <attr name="backgroundColor" />
        <attr name="opacity" />
        <attr name="yTop"/>
    </declare-styleable>

</resources>